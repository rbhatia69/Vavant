<div class="page-header">
<h1 class="page-title">Sign Up</h1>
</div>


<%= semantic_form_for @user, :url => {:action => "create"} do |f| %>

  <%= f.inputs :email, :username, :password, :password_confirmation %>
    <%= f.actions do %>
        <%= f.action :submit, :label => 'Save' %>
    <% end %>

<% end %>

<%= render "links" %>
